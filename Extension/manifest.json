{
    "manifest_version": 2,
    "name": "Fake News Finder",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery.js","content.js"],
            "run_at": "document_end",
            "all_frames": false
        }
		],
    "background": {
        "scripts": ["jquery.js", "background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]

}